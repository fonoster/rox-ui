{
  "name": "rox",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Efra Peralta",
    "email": "efra23peralta@gmail.com",
    "url": "https://efrainperalta.com/"
  },
  "scripts": {
    "apps:voice": "yarn workspace @rox/voice",
    "apps:web": "yarn workspace @rox/web",
    "build": ".scripts/run web:build",
    "fix:styles": "yarn lint:styles --fix",
    "git:branch": ".scripts/run branch-name",
    "git:pre-commit": ".scripts/run lint",
    "git:pre-commit-msg": "commitlint --edit $1",
    "lint:check": "eslint . --ext .js,.ts,.tsx",
    "lint:format": "prettier \"./**/*.{ts,tsx,js}\" --check",
    "lint:ts:check": "tsc --noEmit",
    "prepare": "husky install",
    "setup": ".scripts/run setup",
    "start:voice": ".scripts/run voice:start",
    "start:web": ".scripts/run web:start",
    "test": "jest --forceExit --detectOpenHandles --watch",
    "build:webpack": "webpack",
    "start:webpack": "tsc && webpack"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "js-base64": "^3.7.0",
    "preact": "^10.5.14",
    "simple-websocket": "^9.1.0",
    "wphone": "https://github.com/fonoster/wphone",
    "ws": "^8.1.0",
    "@fonos/googletts": "0.1.11-alpha.4",
    "@fonos/sdk": "0.1.11-alpha.7",
    "@fonos/voice": "0.1.11-alpha.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@nurminen/html-beautify-webpack-plugin": "^1.1.3",
    "@parcel/transformer-inline-string": "^2.0.0-beta.3.1",
    "@parcel/transformer-sass": "2.0.0-rc.0",
    "@testing-library/preact": "^2.0.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.9.1",
    "@types/simple-websocket": "^7.0.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.4",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "parcel": "^2.0.0-beta.3.1",
    "postcss": "^8.2.1",
    "prettier": "^2.3.0",
    "prettier-webpack-plugin": "^1.2.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "svg-fill-loader": "0.0.8",
    "ts-jest": "^27.0.4",
    "ts-loader": "8.3.0",
    "typescript": "^4.3.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": "<=14.x.x"
  },
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  }
}
